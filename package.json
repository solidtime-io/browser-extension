    "@vueuse/core": "^12.5.0",
    "axios": "^1.13.2",
    "dayjs": "^1.11.19",
    "vue": "^3.5.21"
  },
  "devDependencies": {
    "@tailwindcss/container-queries": "^0.1.1",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.19",
    "@wxt-dev/module-vue": "^1.0.2",
    "autoprefixer": "^10.4.22",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.4",
    "typescript": "^5.9.2",
    "vue-tsc": "^2.2.0",
