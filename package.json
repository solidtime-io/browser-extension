{
  "name": "solidtime-browser-extension",
  "description": "Official Browser Extension for solidtime",
  "private": true,
  "version": "0.0.8",
  "type": "module",
  "scripts": {
    "dev": "wxt",
    "dev:firefox": "wxt -b firefox",
